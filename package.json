{
  "name": "license-inspector",
  "version": "1.9.0",
  "description": "This CLI tool helps to read the licenses of maven projects.",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/josejulio/license-inspector"
  },
  "scripts": {
    "test": "mocha specs/*",
